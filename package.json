{
  "name": "react-babel",
  "version": "1.0.0",
  "description": "",
  "main": "build/main.js",
  "scripts": {
    "clean": "rimraf build dist",
    "build-electron": "tsc --outDir build electron/main.ts electron/preload.ts",
    "build-dist": "npm run build-electron && webpack --mode production --progress",
    "prestart": "npm run build-electron",
    "start": "concurrently \"webpack serve --mode development --port 3000\" \"wait-on http://localhost:3000 && electron build/main.js\"",
    "prebuild": "npm run build-dist",
    "build": "electron-builder"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.9",
    "babel-loader": "^9.1.0",
    "concurrently": "^7.5.0",
    "copy-webpack-plugin": "^11.0.0",
    "core-js": "^3.26.1",
    "cross-env": "^7.0.3",
    "electron": "^20.3.5",
    "electron-builder": "^23.6.0",
    "regenerator-runtime": "^0.13.11",
    "rimraf": "^3.0.2",
    "typescript": "^4.9.3",
    "wait-on": "^6.0.1",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.0",
    "webpack-dev-server": "^4.11.1"
  },
  "dependencies": {
    "electron-is-dev": "^2.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "build": {
    "appId": "com.electron.react-babel",
    "win": {
      "icon": "dist/favicon.ico"
    },
    "directories": {
      "buildResources": "public"
    }
  }
}
